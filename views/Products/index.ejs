<%- include('../partials/head') %>

<style>
	a:visited {
		color: blue;
	}
</style>

<h1>All Products!</h1>
<ul>
	<% for (let product of products) { %>

	<li>
		<a href="/products/<%= product._id %> "
			><%= product.name %>
			<span style="color: #ffffff !important;"></span>
		</a>
	</li>

	<%}%>
</ul>
<a href="/products/new">Add Product</a>

